		<UserSecretsId>132c573d-ea30-413d-9176-5f0fe269d568</UserSecretsId>
	</PropertyGroup>
	<PropertyGroup>
		<IncludeOpenAPIAnalyzers>true</IncludeOpenAPIAnalyzers>
