{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "secondaryAzureLocation": {
      "value": "${SECONDARY_AZURE_LOCATION}"
    },
    "isProd": {
      "value": "${IS_PROD}"
    },
    "isRunAsServicePrincipal": {
      "value": "${IS_RUN_AS_SERVICEPRINCIPAL=false}"
    },
    "azureSqlPassword": {
      "value": "${AZURE_SQL_PASSWORD}"
    },
    "frontEndAzureAdB2CApiScope": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} frontEndAzureAdB2CApiScope)"
    },
    "frontEndAzureAdB2cClientId": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} frontEndAzureAdB2cClientId)"
    },
    "frontEndAzureAdB2cClientSecret": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} frontEndAzureAdB2cClientSecret)"
    },
    "apiAzureAdB2cClientId": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} apiAzureAdB2cClientId)"
    },
    "azureAdB2cDomain": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} azureAdB2cDomain)"
    },
    "azureAdB2cInstance": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} azureAdB2cInstance)"
    },
    "azureAdB2cTenantId": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} azureAdB2cTenantId)"
    },
    "azureAdB2cSignupSigninPolicyId": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} azureAdB2cSignupSigninPolicyId)"
    },
    "azureAdB2cResetPolicyId": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} azureAdB2cResetPolicyId)"
    },
    "azureAdB2cSignoutCallback": {
      "value": "$(secretOrRandomPassword ${AZURE_OPS_VAULT_NAME} azureAdB2cSignoutCallback)"
    },
    "enableTelemetry": {
      "value": "${ENABLE_TELEMETRY=true}"
    }
  }
}